{
  "preset": "per",
  "exclude": [
    "packages/coinbase-php"
  ],
  "rules": {
    "group_import": true,
    "ordered_types": {
      "null_adjustment": "always_last"
    },
    "single_import_per_statement": false,
    "trailing_comma_in_multiline": {
      "elements": [
        "arrays",
        "arguments",
        "parameters",
        "match"
      ]
    },
    "void_return": true
  }
}
